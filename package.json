{
  "name": "ecs-pf",
  "version": "2.2.27",
  "description": "CLI for port-forwarding to RDS via AWS ECS",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "ecs-pf": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "aws",
    "ecs",
    "rds",
    "port-forwarding",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "scripts": {
    "connect": "bun run build && bun dist/cli.js connect",
    "exec": "bun run build && bun dist/cli.js exec",
    "enable-exec": "bun run build && bun dist/cli.js enable-exec",
    "build": "./scripts/build.sh",
    "build:tsc": "bunx tsc --project tsconfig.build.json",
    "generate-version": "bun scripts/generate-version.mjs",
    "generate-changelog": "bun scripts/generate-changelog.mjs",
    "changelog:init": "bun scripts/generate-changelog.mjs init",
    "changelog:update": "bun scripts/generate-changelog.mjs generate",
    "start": "bun dist/cli.js connect",
    "type-check": "tsc --noEmit",
    "test": "bun run type-check && bun run build && bun test",
    "test:unit": "bun test test/unit",
    "test:integration": "bun test test/integration",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "prepublishOnly": "bun run check",
    "format": "biome format --write",
    "format:check": "biome format",
    "lint": "biome lint --write",
    "lint:check": "biome lint",
    "check": "biome check --write",
    "check:dry": "biome check",
    "ci": "biome ci"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@types/inquirer": "^9.0.8",
    "conventional-changelog-cli": "^4.1.0",
    "conventional-changelog-conventionalcommits": "^9.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.830.0",
    "@aws-sdk/client-ecs": "^3.830.0",
    "@aws-sdk/client-rds": "^3.830.0",
    "@aws-sdk/client-ssm": "^3.830.0",
    "@inquirer/prompts": "^7.5.3",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "fuse.js": "^7.1.0",
    "inquirer": "^12.6.3",
    "inquirer-search-list": "^1.2.6",
    "remeda": "^2.23.1",
    "valibot": "^1.1.0"
  }
}
